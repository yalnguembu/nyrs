{
  "name": "quonte-api",
  "version": "0.1.0",
  "description": "serveur d'api de ghostbarber qui va servir les donn√©es au client sous forme d'api rest",
  "main": "index.js",
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-logger": "^0.0.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.0.8",
    "multer": "^1.4.4",
    "prisma": "^4.14.1",
    "socket.io": "^4.5.1"
  },
  "scripts": {
    "build": "npx tsc",
    "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/main.js\"",
    "start:ci": "node ./dist/src/main.js",
    "prisma:migrate:ci": "npx prisma migrate deploy",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "db:stop:dev": "docker compose rm quonte-db -s -f -v",
    "db:start:dev": "docker compose up quonte-db -d",
    "db:restart:dev": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
    "start:all:dev": "npm run db:start:dev && npm run start:dev"
  },
  "author": "Yal Nguembu ",
