  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-arrow-functions": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.13",
    "rewire": "^6.0.0",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.9.5"
  }
}